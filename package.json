{
  "name": "lpac",
  "version": "1.3.1",
  "description": "WooCommerce plugin to allow customers to choose their location at checkout.",
  "dependencies": {},
  "devDependencies": {
    "parcel": "^2.0.0-rc.0"
  },
  "scripts": {
    "watch": "parcel watch --no-hmr",
    "build": "parcel build"
  },
  "targets": {
    "lite-maps": {
      "source": ["./assets/public/js/maps/base-map.js", "./assets/public/js/maps/checkout-page-map.js", "./assets/public/js/maps/order-received-map.js", "./assets/public/js/maps/order-details-map.js"],
      "sourceMap": false,
      "distDir": "./assets/public/js/maps/build/"
    },
    "pro-maps": {
      "source": ["./assets/pro/public/js/maps/checkout-page-map-pro.js"],
      "sourceMap": false,
      "distDir": "./assets/pro/public/js/maps/build/"
    }
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "outputFormat" : "global",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UVLabs/location-picker-at-checkout-for-woocommerce.git"
  },
  "author": "Uriahs Victor",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/UVLabs/location-picker-at-checkout-for-woocommerce/issues"
  },
  "homepage": "https://github.com/UVLabs/location-picker-at-checkout-for-woocommerce#readme"
}
